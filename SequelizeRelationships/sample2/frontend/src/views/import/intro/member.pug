extends ../../common/base.pug

block title
  title 구성원
block append stylesheet
  link(rel="stylesheet" href="/css/member.css")
block append script
  script(defer src="/es5/member.js")

block content
  main.sub-main
    .sub-background
    .sub-main__wrap

      // 여기서부터 네비게이션 바 시작
      .sub-nav
        .sub-nav__title-wrap
          h3.sub-nav__title 학과소개
        .sub-nav__list-wrap
          ul.sub-nav__list
            li.sub-nav__item
              a(href="/intro/info").sub-nav__link 학과소개
            li.sub-nav__item
              a(href="/intro/member").sub-nav__link 구성원
            li.sub-nav__item
              a(href="/intro/env").sub-nav__link 교육 환경
            li.sub-nav__item
              a(href="/intro/club").sub-nav__link 동아리

      // 여기서부터 콘텐츠 시작
      .sub-contents
        .sub-contents__wrap
          .sub-contents__header
            h2.sub-contents__title 구성원
            .sub-contents__util-wrap
              ul.sub-contents__util-list
                li.sub-contents__item
                  +sub-contents__item__icon("home")
                  a(href="/").sub-contents__item__link HOME
                  +sub-contents__item__icon("chevronForward") 
                li.sub-contents__item
                  a(href="/intro/info").sub-contents__item__link 학과소개
                  +sub-contents__item__icon("chevronForward")
                li.sub-contents__item
                  a(href="/intro/member").sub-contents__item__link 구성원

          .sub-contents__content
            .sub-contents__member-wrap
              ul.sub-contents__member-list
                li.sub-contents__member-item
                  .sub-contents__member-image-zone
                    .sub-contents__member-image.sub-contents__lee-image
                  .sub-contents__member-profile
                    h3.sub-contents__member-name 이권일 교수
                    ul.sub-contents__member-info-list
                      li.sub-contents__member-info-item
                        span.sub-contents__member-info-item__title 직위
                        span.sub-contents__member-info-item__info 교수
                      li.sub-contents__member-info-item
                        span.sub-contents__member-info-item__title 담당과목
