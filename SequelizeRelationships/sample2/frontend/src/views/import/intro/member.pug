                      li.sub-contents__member-info-item
                        span.sub-contents__member-info-item__title 전화번호
                        span.sub-contents__member-info-item__info 042-866-0398
                      li.sub-contents__member-info-item
                        span.sub-contents__member-info-item__title 연구실
