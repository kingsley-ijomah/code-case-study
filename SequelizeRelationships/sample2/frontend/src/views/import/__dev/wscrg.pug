extends ../../common/base.pug

block title
  title 지용킹의 개발공간
block append stylesheet
  link(rel="stylesheet" href="/css/wscrg.css")
block append script
  script(defer src="/es5/wscrg.js")

block content
  main.sub-main
    .sub-background
    .sub-main__wrap

      // 여기서부터 네비게이션 바 시작
      .sub-nav
        .sub-nav__title-wrap
          h3.sub-nav__title 학과소개
        .sub-nav__list-wrap
          ul.sub-nav__list
            li.sub-nav__item
              a(href="#").sub-nav__link 학과소개
            li.sub-nav__item
              a(href="#").sub-nav__link 구성원
            li.sub-nav__item
              a(href="#").sub-nav__link 교육환경
            li.sub-nav__item
              a(href="#").sub-nav__link 동아리



      // 여기서부터 콘텐츠 시작
      .sub-contents
        .sub-contents__wrap
          .sub-contents__header
            h2.sub-contents__title 학과소개
            .sub-contents__util-wrap
              ul.sub-contents__util-list
                li.sub-contents__item
                  +sub-contents__item__icon("home")
                  a(href="#").sub-contents__item__link HOME
                  +sub-contents__item__icon("chevronForward") 
                li.sub-contents__item
                  a(href="#").sub-contents__item__link 학과소개
                  +sub-contents__item__icon("chevronForward")
                li.sub-contents__item
                  a(href="#").sub-contents__item__link 학과소개

          .sub-contents__content
            +postWrite()